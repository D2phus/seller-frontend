.machines-list{
  display: flex;
  flex-direction: column;
  margin: 20rpx 20rpx 20rpx 30rpx;
  padding: 25rpx;
  background: white;
}
.row{
  display: flex;
  width: 100%;
  justify-content: start;
  height: 1.5rem;
  align-items: center;
}
.label{
  width: 40%;
  justify-content: center;
  text-align: start;
  color: grey;

  font-size: 34rpx;
}
.content{
  justify-content: center;
  text-align: start;

  font-size: 34rpx;
}
.tr{
  display: flex;
  width: 100%;
  justify-content: start;
  height: 1rem;
  align-items: center;
  padding-left: 40%; /* 使内容表格和其他数据对齐 */
  
  font-size: 34rpx;
}
.td{
  width: 40%;
  justify-content: center;
  text-align: start;
}
